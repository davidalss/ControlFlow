# Configurações do Banco de Dados
DATABASE_URL=postgresql://controlflow_db:123@postgres:5432/controlflow_db

# Configurações de Autenticação
JWT_SECRET=controlflow-jwt-secret-key-2024-development
SESSION_SECRET=controlflow-session-secret-2024

# Configurações do Servidor
PORT=5002
NODE_ENV=development

# Configurações do Gemini AI (Severino)
GEMINI_API_KEY=your-gemini-api-key-here

# Configurações de Upload
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760

# Configurações de Email (opcional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Configurações de Log
LOG_LEVEL=info

# Configurações do Redis (opcional)
REDIS_URL=redis://redis:6379
