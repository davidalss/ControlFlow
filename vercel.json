{
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://enso-backend-0aa1.onrender.com/api/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://enso-backend-0aa1.onrender.com"
  }
}
