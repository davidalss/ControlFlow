{
  "name": "enso-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "check-env": "node scripts/check-env-vars.js",
    "prebuild:check": "npm run check-env",
    "build:safe": "npm run check-env && npm run build",
    "build:netlify:safe": "npm run check-env && npm run build",
    "build:render": "npm run check-env && npm run build",
    "build:render:safe": "npm run check-env && npm run build",
    "test:diagnostic": "node ../scripts/test-frontend-issues.js",
    "test:api": "node ../scripts/check-api.js",
    "test:api-auth": "node ../scripts/test-api-with-login.js",
    "fix:imports": "node ../scripts/fix-imports.js",
    "diagnostic:run": "echo 'Executando diagnóstico automático...' && npm run dev",
    "test:all": "npm run test:api && npm run test:api-auth && npm run test:diagnostic",
    "fix:all": "npm run fix:imports && npm run lint",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch",
    "test:unit:coverage": "jest --config jest.config.js --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:error-scenarios": "jest --config jest.config.js --testPathPattern=error-scenarios.test.tsx",
    "test:integration": "npm run test:unit && npm run test:e2e",
    "test:full": "npm run test:all && npm run test:integration",
    "logs:export": "node -e \"console.log(JSON.stringify(window.logger?.getAllLogs() || [], null, 2))\"",
    "logs:clear": "node -e \"window.logger?.clearLogs()\"",
    "logs:report": "node -e \"console.log(JSON.stringify(window.logger?.getErrorReport() || {}, null, 2))\"",
    "monitor:start": "npm run dev & npm run test:e2e:headed",
    "monitor:logs": "npm run logs:report && npm run test:error-scenarios"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.7",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-hover-card": "^1.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-menubar": "^1.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.6",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.60.5",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cropperjs": "^1.6.2",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "input-otp": "^1.4.2",
